#include <stdio.h>
int main()
 {
 	int input;
 	printf("Enter a positive number:");
    scanf("%d",&input);
    if(input%2==0)
    {
    	printf("Even");
    } else
     {
     	printf("Odd");
     }
    return 0;
}
#include <stdio.h>
int main()
 {
 	char ch;
 	printf("Enter any letter:");
    scanf("%c",&ch);
    if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u')
    {
    	printf("Vowel");
    } else
     {
     	printf("Consonent");
     }
    return 0;
}
